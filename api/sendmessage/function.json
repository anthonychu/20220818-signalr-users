{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "signalR",
      "direction": "out",
      "name": "message",
      "hubName": "chat",
      "connectionStringSetting": "AzureSignalRConnectionString"
    }
  ]
}
